{
    "version": 2,
    "builds": [
      {
        "src": "build/web/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "build/web/index.html"
      }
    ],
    "build": {
      "command": "bash install_flutter.sh && flutter pub get && flutter build web --release"
    },
    "outputDirectory": "build/web"
  }